<?xml version="1.0"?>
<launch>
    <!-- <include file="$(find realsense2_camera)/launch/rs_t265.launch"/> -->
    
    <arg name="config_file" default="$(find vins)/../config/realsense_t265/t265.yaml" />

    <node name="vins_estimator" pkg="vins" type="vins_node" output="screen" >
